    \entry{abumandour_applying_2021}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=1432e031f149f4b23cc36505e5800562}{%
           family={Abumandour},
           familyi={A\bibinitperiod},
