    \entry{de_la_hoz_self-explanation_2023}{article}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=e499c52438f2b0c56fd36bde70946f51}{%
           family={De\bibnamedelima La\bibnamedelima Hoz},
