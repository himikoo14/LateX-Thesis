      \strng{namehash}{b3f9965a65e9dba24023c03d142c6bf3}
      \strng{fullhash}{f1f52b3ce903d74f3a9bc6b90446a458}
      \strng{fullhashraw}{f1f52b3ce903d74f3a9bc6b90446a458}
      \strng{bibnamehash}{b3f9965a65e9dba24023c03d142c6bf3}
      \strng{authorbibnamehash}{b3f9965a65e9dba24023c03d142c6bf3}
      \strng{authornamehash}{b3f9965a65e9dba24023c03d142c6bf3}
      \strng{authorfullhash}{f1f52b3ce903d74f3a9bc6b90446a458}
