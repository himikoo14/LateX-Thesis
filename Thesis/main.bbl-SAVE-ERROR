      \strng{namehash}{a6b8e3d7847d9c5d660ccc860953f26d}
      \strng{fullhash}{a6b8e3d7847d9c5d660ccc860953f26d}
      \strng{fullhashraw}{a6b8e3d7847d9c5d660ccc860953f26d}
      \strng{bibnamehash}{a6b8e3d7847d9c5d660ccc860953f26d}
      \strng{authorbibnamehash}{a6b8e3d7847d9c5d660ccc860953f26d}
      \strng{authornamehash}{a6b8e3d7847d9c5d660ccc860953f26d}
